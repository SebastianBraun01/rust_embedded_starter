{
  "version": "2.0.0",
  "tasks": [
        {
            "label": "cargo: build",
            "type": "cargo",
            "command": "build",
            "problemMatcher": ["$rustc"],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "cargo: flash",
            "type": "cargo",
            "command": "embed",
            "problemMatcher": ["$rustc"],
            "group": "build"
        },
        {
            "label": "cargo: clean",
            "type": "cargo",
            "command": "clean",
            "problemMatcher": ["$rustc"],
            "group": "build"
        }
    ]
}
